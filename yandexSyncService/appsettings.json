{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceSettings": {
    "WorkerRestartDelayDays": 3
  },
  "UserDbSettings": {
    "DataBaseName": "[user mongo database name]",
    "CollectionName": "[user mongo collection]"
  },
  "EventDbSettings": {
    "DataBaseName": "[event mongo database name]",
    "CollectionName": "[event mongo collection]"
  },
  "ConnectionStrings": {
    "MongoConnectionString": "[Mongo connection string]"
  },
  "RabbitMqSettings": {
    "Url": "rabbit url",
    "User": "[rabbit user]",
    "Password": "[rabbit password]",
    "TimeoutMs": 500
  },
  "YandexSettings": {
    "Token": "[yandex token]",
    "UsersPerPage": 1000,
    "UsersRequestUrl": "[yandex request url]"
  }
}


